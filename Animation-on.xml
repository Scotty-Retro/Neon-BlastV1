<theme>
   <formatVersion>7</formatVersion>

<view name="system">

         <image name="wallpaper1" extra="true">
	     <storyboard>
         <animation property="y" from="-2" begin="0" duration="0" mode="easyOut"/>
         <animation property="scale" to="1.2" begin="350" autoreverse="true" duration="3200" repeat="forever" mode="easyIn"/>
         <animation property="opacity" from="0" to="1" duration="350" mode="linear"/>
		 </storyboard>
         </image>

	   <image name="brightwallpapers" extra="true">
	     <storyboard>
         <animation property="y" from="-2" begin="0" duration="0" mode="easyOut"/>
         <animation property="scale" to="1.2" begin="350" autoreverse="true" duration="3200" repeat="forever" mode="easyIn"/>
         <animation property="opacity" from="0" to="1" duration="350" mode="linear"/>
		 </storyboard>
         </image>

	   <image name="darkwallpapers" extra="true">
	     <storyboard>
         <animation property="y" from="-2" begin="0" duration="0" mode="easyOut"/>
         <animation property="scale" to="1.2" begin="350" autoreverse="true" duration="3200" repeat="forever" mode="easyIn"/>
         <animation property="opacity" from="0" to="1" duration="350" mode="linear"/>
		 </storyboard>
         </image>
		 
	  <image name="char" extra="true">
		<storyboard> 
        <animation property="y" from="1.01" begin="0" duration="1000" mode="Bump"/>
        <animation property="scale" to="1.02" begin="1100" autoreverse="true" duration="300" repeat="forever" mode="easyIn"/>
		</storyboard>
          </image>

	  <image name="char1" extra="true">
           <storyboard>
           <animation property="x" from="-2" to="0.045" duration="2100" autoreverse="false" begin="-2"/>
           </storyboard>
          </image>

	<image name="menu2frames" extra="true">
	<storyboard> 
             <animation property="y" from="-1.647777" to="0.5" duration="2089.5" autoreverse="false" begin="1.5" mode="linear"/>
	</storyboard>
	</image>

	<video name="backgroundvideom2" extra="true">
	<storyboard> 
             <animation property="y" from="-1.647999" to="0.4" duration="2099" autoreverse="false" begin="1.5" mode="linear"/>
	</storyboard>
	</video>

</view>


<view name="basic, detailed, video, grid, gamecarousel">
	

	</view>


